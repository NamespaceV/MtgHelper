<Window x:Class="ScryfallAPI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScryfallAPI"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="300">
    <Grid>
        <DockPanel>
            <Button DockPanel.Dock="Top" Margin="10,10,10,0" VerticalAlignment="Top" Command="{Binding DownloadSetCommand}">Download Set</Button>
            <Button DockPanel.Dock="Top" Margin="10,10,10,0" VerticalAlignment="Top" Command="{Binding FindMissingCommand}">Find missing</Button>
        </DockPanel>
    </Grid>
</Window>
